@model List<EducationPortal.Web.Models.User>

	<h2>Öğrenci Listesi</h2>

	<table class = "table table-striped">
		<thead>
			<tr>
				<th>Ad Soyad</th>
				<th>E-posta</th>
			</tr>
		</thead>
		<tbody>
		@foreach(var student in Model)
		{
			<tr>
				<td>@student.FullName</td>
				<td>@student.Email</td>
			</tr>
		}
	</tbody>
	</table>